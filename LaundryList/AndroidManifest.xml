<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.laundrylist"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="16"
        android:targetSdkVersion="17" />

    <uses-permission android:name="android.permission.INTERNET" >
    </uses-permission>

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="laundrylist.view.LoginActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="laundrylist.view.ExploreActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="laundrylist.view.GoalActivity1"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="laundrylist.view.GoalActivity2"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="laundrylist.view.GoalActivity3"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="laundrylist.view.GoalFocusActivity"
            android:label="@string/app_name" >
        </activity>
    </application>

</manifest>